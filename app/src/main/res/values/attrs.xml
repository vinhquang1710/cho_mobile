<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomFontTextView">
        <attr name="typeface"/>
    </declare-styleable>

    <attr name="typeface" format="enum">
        <enum name="roboto_thin" value="0"/>
        <enum name="roboto_light" value="1"/>
        <enum name="roboto_regular" value="2"/>
        <enum name="roboto_italic" value="3"/>
        <enum name="roboto_medium" value="4"/>
        <enum name="roboto_bold" value="5"/>
    </attr>
</resources>